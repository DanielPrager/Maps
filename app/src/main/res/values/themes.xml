<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Formular" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00c853</item>
        <item name="colorPrimaryVariant">#5efc82</item>
        <item name="colorOnPrimary">#009624</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#ffb300</item>
        <item name="colorSecondaryVariant">#ffe54c</item>
        <item name="colorOnSecondary">#c68400</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>
